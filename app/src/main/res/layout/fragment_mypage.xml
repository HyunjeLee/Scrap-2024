<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".nav.MypageFragment">

    <TextView
        android:id="@+id/textMypage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="19dp"
        android:includeFontPadding="false"
        android:text="마이페이지"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="91dp"
        android:includeFontPadding="false"
        android:text="지금까지 스크랩한 개수"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="308dp"
        android:layout_height="135dp"
        android:layout_marginTop="127dp"
        android:src="@drawable/bg_mp_all_round_25dp_with_light_gray_stroke"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:importantForAccessibility="no" />

    <TextView
        android:id="@+id/imageView2"
        android:layout_width="80dp"
        android:layout_height="106dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/bg_all_round_25dp"
        android:backgroundTint="@color/main_heavy"
        android:fontFamily="@font/notosanskr_bold"
        android:gravity="center"
        android:includeFontPadding="false"
        android:paddingBottom="20dp"
        android:text="9"
        android:textColor="@color/white"
        android:textSize="70sp"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/imageView3"
        android:layout_width="80dp"
        android:layout_height="106dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/bg_all_round_25dp"
        android:backgroundTint="@color/main_heavy"
        android:gravity="center"
        android:includeFontPadding="false"
        android:paddingBottom="20dp"
        android:text="9"
        android:fontFamily="@font/notosanskr_bold"
        android:textColor="@color/white"
        android:textSize="70sp"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/imageView4"
        android:layout_width="80dp"
        android:layout_height="106dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/bg_all_round_25dp"
        android:backgroundTint="@color/main_heavy"
        android:gravity="center"
        android:includeFontPadding="false"
        android:paddingBottom="20dp"
        android:text="9"
        android:fontFamily="@font/notosanskr_bold"
        android:textColor="@color/white"
        android:textSize="70sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="305dp"
        android:text="공지사항"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="67dp"
        android:layout_height="22dp"
        android:layout_marginStart="121dp"
        android:layout_marginBottom="69dp"
        android:background="@null"
        android:includeFontPadding="false"
        android:text="로그아웃"
        android:textSize="18sp"
        android:textColor="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="67dp"
        android:layout_height="22dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="69dp"
        android:background="@null"
        android:includeFontPadding="false"
        android:text="회원탈퇴"
        android:textColor="@color/gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button" />
</androidx.constraintlayout.widget.ConstraintLayout>